<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.toray.ojt.web.mapper.UserDetailsMapper">
    <select id="findUserNameByPartyId" parameterType="String" resultType="com.toray.ojt.web.entity.UserDetails">
        SELECT PARTY_ID, PARTY_NAME_KJ, PARTY_NAME_KN, PARTY_NAME_EN
        FROM BASE_PARTY
        WHERE PARTY_ID = #{partyId}
    </select>
</mapper>